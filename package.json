{
  "name": "angular2-webpack2-starter",
  "version": "0.0.0",
  "description": "An Angular 2 Starter kit featuring Angular 2 and Webpack 2",
  "author": "PatrickJS <github@gdi2290.com>",
  "main": "index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev -- --env.HMR",
    "server:dev": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.ENV development",
    "build": "node_modules/webpack/bin/webpack.js --env.ENV development",
    "typings-install": "node_modules/typings/dist/bin.js install",
    "postinstall": "npm run typings-install"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
