    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@types/core-js": "^0.9.22-alpha",
    "@types/node": "^4.0.22-alpha",
    "core-js": "^2.2.0",
    "rxjs": "~5.0.0-beta.2",
    "typescript": "^1.9.0-dev.20160607-1.0",
    "typings": "^1.0.4",
    "zone.js": "~0.6.12"
  },
  "devDependencies": {
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "json-loader": "^0.5.4",
